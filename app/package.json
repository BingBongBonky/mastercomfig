{
  "name": "mastercomfig-app",
  "version": "7.0.0-a.7",
  "description": "The mastercomfig app allows you to customize and optimize Team Fortress 2 easily.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder -wlp \"always\""
  },
  "repository": "github:mastercoms/mastercomfig",
  "keywords": [
    "tf2",
    "team-fortress-2",
    "valve",
    "tf2-config",
    "source-engine"
  ],
  "author": {
    "name": "mastercoms",
    "email": "mastercoms@tutanota.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastercoms/mastercomfig/issues"
  },
  "homepage": "https://github.com/mastercoms/mastercomfig#readme",
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^2.21.10",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "speedtest-net": "^1.4.3",
    "xel": "0.0.139"
  },
  "build": {
    "productName": "mastercomfig",
    "appId": "com.mastercomfig.app",
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "synopsis": "The desktop app for mastercomfig, a Team Fortress 2 config.",
      "target": [
        "snap",
        "deb",
        "rpm",
        "pacman",
        "tar.xz"
      ],
      "category": "Settings"
    },
    "snap": {
      "confinement": "classic",
      "grade": "devel"
    },
    "win": {
      "compression": "maximum",
      "target": [
        {
          "target": "nsis-web",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "legalTrademarks": "Valve, the Valve logo, Steam, the Steam logo, Team Fortress, the Team Fortress logo are trademarks and/or registered trademarks of Valve Corporation. mastercomfig is not associated with nor endorsed by Valve Corporation."
    }
  }
}
